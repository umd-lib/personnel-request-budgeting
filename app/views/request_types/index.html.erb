<div class="page-header">
  <h1>Request Types</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Code</th>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @request_types.each do |request_type| %>
        <tr>
          <td><%= request_type.code %></td>
          <td><%= request_type.name %></td>
          <%= render 'shared/table_action_buttons', object: request_type %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<br>

<%= link_to 'New Request type', new_request_type_path %>
